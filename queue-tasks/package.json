    "watch": "tsc --build -w",
    "serve": "nodemon ./build/index.js"
  },
  "dependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/node": "^16.9.1",
    "@types/sqs-consumer": "^5.0.0",
    "aws-sdk": "^2.987.0",
    "nodemon": "^2.0.12",
    "npm-run-all": "^4.1.5",
    "sqs-consumer": "^5.6.0"
  },
  "devDependencies": {
    "typescript": "^4.4.3"
  }
}
